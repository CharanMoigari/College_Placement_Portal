<!DOCTYPE html>
<html>
<head>
  <title>Previous Years Placements</title>
  <link rel="stylesheet" type="text/css" href="/css/footer.css">
  <link rel="stylesheet" type="text/css" href="/css/headerwithout.css">
  <link rel="stylesheet" type="text/css" href="/css/homeHeader.css">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
  <style>
    table {
      width: 100%;
      border-collapse: collapse;
    }

    th, td {
      padding: 8px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }

    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
    <%- include('partials/homeHeader.ejs') %>
  <h1 style="text-align: center;">Previous Years Placements</h1>
  <table>
    <thead>
      <tr>
        <th>Company Name</th>
        <th>Package</th>
        <th>CSE</th>
        <th>ECE</th>
        <th>IT</th>
        <th>Mech</th>
        <th>Civil</th>
        <th>EEE</th>
        <th>MBA</th>
      </tr>
    </thead>
    <% for (let i = 0; i < 100; i++) { %>
      <tbody>
        <tr>
          <td>Company <%= i+1 %></td>
          <td>9 LPA</td>
          <td>15</td>
          <td>10</td>
          <td>1</td>
          <td>3</td>
          <td>9</td>
          <td>5</td>
          <td>NA</td>
        </tr>
        <!-- Add more rows for each placement data -->
      </tbody>
    <% } %>
  </table>
  <%- include('partials/footer.ejs') %>
</body>
</html>
